      # waive the privileges and immunities granted to it by virtue of its status
      # as an Intergovernmental Organization or submit itself to any jurisdiction.
    '''
  "autocomplete-python":
    useKite: false
  core:
    disabledPackages: [
      "goto-definition"
    ]
    telemetryConsent: "no"
  editor:
    invisibles: {}
    showIndentGuide: true
    showInvisibles: true
    tabType: "soft"
  "exception-reporting":
    userId: "264c1100-c526-07df-3ca3-2ff21d502625"
  "git-blame":
    columnWidth: 311
  "linter-ui-default": {}
  "markdown-preview":
    breakOnSingleNewline: true
  "project-manager":
    alwaysOpenInSameWindow: true
    includeGitRepositories: true
  "tree-view": {}
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
