"*":
  "atom-beautify":
    css:
      beautify_on_save: true
      indent_size: 2
      newline_between_rules: false
      preserve_newlines: true
      selector_separator_newline: true
    general:
      _analyticsUserId: "79831183-0c13-40f4-abe0-701b0dd6ba67"
    html:
      indent_size: 2
  "auto-copyright":
    owner: "CERN"
    template: '''
      # -*- coding: utf-8 -*-
      #
      # This file is part of Invenio.
      # Copyright (C) %y %o.
